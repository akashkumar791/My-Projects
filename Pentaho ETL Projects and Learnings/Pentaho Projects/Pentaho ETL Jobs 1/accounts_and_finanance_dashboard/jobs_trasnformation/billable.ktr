    <sql>select d.ddo_employee_id,f.name,d.month,c.name,e.firstname,e.lastname,p.name,SUM&#x28;d.abs_allocation_perc&#x29; as content&#xa;from ddo_fyear f,ddo_project_allocation_monthwise d&#xa;left join ddo_employee e on e.ddo_employee_id &#x3d; d.ddo_employee_id&#xa;left join ddo_project p on p.ddo_project_id &#x3d; d.ddo_project_id&#xa;left join ddo_client c on c.ddo_client_id &#x3d; d.ddo_client_id&#xa;where d.ddo_employee_id IN &#x28;select distinct ddo_employee_id from ddo_empworkdetails where isbillable &#x3d; &#x27;Y&#x27;&#x29;&#xa;and d.ddo_employee_id not IN &#x28;&#x28;select distinct ddoe.ddo_employee_id&#xa;from ddo_employee ddoe&#xa;left join ddo_empworkdetails ddoew on ddoe.ddo_employee_id &#x3d; ddoew.ddo_employee_id&#xa;where ddoe.isactive &#x3d; &#x27;Y&#x27; and ddoew.designation &#x3d; &#x27;1000255&#x27; or ddoew.designation &#x3d; &#x27;1000259&#x27; or ddoew.designation &#x3d; &#x27;1000297&#x27; or ddoew.designation &#x3d; &#x27;1000302&#x27;&#x29;&#x29;&#xa;and month &#x3d; cast&#x28;EXTRACT&#x28;month FROM &#x28;current_date - interval &#x27;0 month&#x27;&#x29;&#x29; as integer&#x29;&#xa;and d.abs_allocation_perc NOT IN &#x28;select d.abs_allocation_perc from ddo_project_allocation_monthwise where d.ddo_project_id &#x3d; 1000096 and d.abs_allocation_perc &#x3d; 0&#x29; &#xa;and f.startdate &#x3c;&#x3d; &#x28;current_date - interval &#x27;0 month&#x27;&#x29; and f.enddate &#x3e;&#x3d; &#x28;current_date - interval &#x27;0 month&#x27;&#x29; and  f.isprocessed &#x3d; &#x27;Y&#x27;&#xa;and p.name &#x21;&#x3d; &#x27;Bench&#x27;&#xa;group by d.ddo_employee_id,c.name,e.firstname,e.lastname,p.name,d.month,f.name&#xa;order by c.name,f.name,d.month,concat&#x28;e.firstname,&#x27; &#x27;,e.lastname&#x29;,p.name&#x3b;&#xa;</sql>
